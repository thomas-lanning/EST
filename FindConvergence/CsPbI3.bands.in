 &control
    calculation = 'bands'
    restart_mode='from_scratch',
    prefix='cspbi3',
    pseudo_dir = './',
    outdir='./cspbi3/'
 /
 &system
! Enter crystal structure information below
    ibrav= 
    celldm(1) = 
    ntyp= 
    nat= 
! Edit plane wave cutoff below if necessary
    ecutwfc = 20.0,
 /
 &electrons
    mixing_mode = 'plain'
    mixing_beta = 0.7
    conv_thr =  1.0d-7
 /
ATOMIC_SPECIES
Cs  132.9 Cs.pz-hgh.UPF
Pb  207.2 Pb.pz-hgh.UPF
I   126.9  I.pz-hgh.UPF
K_POINTS crystal_b   ! Enter appropriate path below
